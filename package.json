{
  "name": "crossing-minds",
  "version": "0.1.0",
  "description": "React+Redux",
  "homepage": "https://github.com/nexton-labs/crossing-minds",
  "author": "nextonlabs",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "npm run validate",
    "build:local": "cross-env NODE_ENV=development webpack --config webpack.local.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "prebuild:analyze": "webpack --config webpack.prod.js --profile --json > buildlog/stats.json",
    "build:analyze": "webpack-bundle-analyzer buildlog/stats.json dist -l debug",
    "prestart": "npm run validate",
    "prestart:local": "npm run validate",
    "start": "npm run start:local",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --open --config webpack.dev.js",
    "start:local": "cross-env NODE_ENV=local webpack-dev-server --open --config webpack.local.js",
    "lint": "eslint --ext .js --ext .jsx --fix ./src",
    "lint:debug": "eslint --ext .js --ext .jsx --fix ./src --debug",
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "prevalidate": "rimraf buildlog",
    "validate": "run-p -c lint test:coverage"
  },
  "dependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@rematch/core": "^2.0.0-next.1",
    "axios": "0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "7.0.2",
    "fontsource-roboto": "^3.0.3",
    "js-logger": "1.6.0",
    "jwt-decode": "2.2.0",
    "moment": "^2.27.0",
    "prettier": "^2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-inputs-validation": "^4.4.5",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-redux": "4.0.8",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "styled-components": "^5.1.1",
    "terser-webpack-plugin": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@fortawesome/fontawesome-free": "5.14.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "axios-mock-adapter": "1.18.2",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.2.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "21.0.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.0",
    "jest-html-reporter": "3.1.3",
    "npm-run-all": "4.1.5",
    "react-hot-loader": "4.12.21",
    "redux-mock-store": "1.5.4",
    "style-loader": "1.2.1",
    "webapp-webpack-plugin": "2.7.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.1"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ]
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
